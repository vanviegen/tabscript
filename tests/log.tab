tabscript 2.0
import plugin "./log-plugin.tab"

# Test @log with a function
@log add := |a: number, b: number| a + b

# Test @log with a non-function value
@log value := 42

# Test @log with a more complex expression
@log multiply := |x: number, y: number| x * y

# Regular function for comparison
normalFunc := |x: number| x * 2

# Call the logged functions
result := add(1, 2)
console.log.. "Result: ${result}"

doubled := multiply(3, 4)
console.log.. "Multiplied: ${doubled}"
