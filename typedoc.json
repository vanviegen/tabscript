{
    "name": "TabScript",
    "readme": "README.md",
    "entryPoints": ["src/*.ts"],
    "projectDocuments": ["docs/*.md"],
    "excludeInternal": true,
    "excludePrivate": true,
    "includeVersion": true,
    "router": "structure-dir",
    "highlightLanguages": [],
    "ignoredHighlightLanguages": ["tabscript", "tab", "typescript", "ts", "javascript", "js", "bash", "sh", "html", "css", "json"],
    "searchInComments": true,
    "searchInDocuments": true,
    "headings": {
        "readme": false
    },
    "plugin": [],
    "theme": "default",
    "navigationLinks": {
        "What's this?": "https://tabscript.vanviegen.net/",
        "Tutorial": "https://tabscript.vanviegen.net/TabScript_Tutorial/",
        "GitHub": "https://github.com/vanviegen/tabscript"
    },
    "hideGenerator": true,
    "hostedBaseUrl": "https://tabscript.vanviegen.net/",
    "useFirstParagraphOfCommentAsSummary": true,
    "validation": {
        "notDocumented": false,
        "notExported": false
    },
    "treatWarningsAsErrors": true,
    "out": "./dist-docs",
    "customFooterHtml": "<script defer src=\"/assets/livecode/livecode.js\" type=\"module\"></script>"
}
