{
  "name": "tabscript",
  "displayName": "TabScript",
  "description": "TabScript language support - indentation-based TypeScript for DSLs",
  "version": "2.0.0",
  "publisher": "tabscript",
  "repository": {
    "type": "git",
    "url": "https://github.com/vanviegen/tabscript"
  },
  "license": "MIT",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "keywords": [
    "tabscript",
    "typescript",
    "indentation",
    "dsl",
    "transpiler"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "tabscript",
        "aliases": [
          "TabScript",
          "tabscript"
        ],
        "extensions": [
          ".tab"
        ],
        "configuration": "./language-configuration.json",
        "icon": {
          "light": "./icon.svg",
          "dark": "./icon.svg"
        }
      }
    ],
    "grammars": [
      {
        "language": "tabscript",
        "scopeName": "source.tabscript",
        "path": "./syntaxes/tabscript.tmLanguage.json"
      }
    ],
    "configuration": {
      "title": "TabScript",
      "properties": {
        "tabscript.trace.server": {
          "type": "string",
          "enum": [
            "off",
            "messages",
            "verbose"
          ],
          "default": "off",
          "description": "Traces the communication between VS Code and the language server."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "lint": "eslint src --ext ts"
  },
  "dependencies": {
    "vscode-languageclient": "^9.0.1",
    "vscode-languageserver": "^9.0.1",
    "vscode-languageserver-textdocument": "^1.0.11"
  }
}